                <literal_block language="ipython3" linenos="False" xml:space="preserve">
                    y = sym.Function('y')
                    n = sym.symbols(r'\alpha')
                    f = y(n)-2*y(n-1/sym.pi)-5*y(n-2)
                    sym.rsolve(f,y(n),[1,4])
